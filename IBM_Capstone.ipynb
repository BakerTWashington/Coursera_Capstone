   "source": [
    "lex_zoning_zip_url = \"https://data.lexingtonky.gov/dataset/d99942a6-2828-496b-b675-82d3cb94e54d/resource/434a9cd5-f7b4-4af4-94a0-038d020081f2/download/zoning.zip\"\n",
